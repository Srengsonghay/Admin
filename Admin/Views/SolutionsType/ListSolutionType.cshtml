@model List<Admin.Models.SolutionsType>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    Layout = "_LayoutDash";

}



<div class="col-lg-12 grid-margin stretch-card">
                <div class="card">
                  <div class="card-body">
                    <h4 class="card-title">
        
           Solutions Type <span style="Float: right;"><a asp-area="" asp-controller="SolutionsType" asp-action="CreateSolutionType">      
           <button type="button" class="btn btn-inverse-primary btn-md" >Create</button></a></span></h4>
            
            
                    <table class="table table-striped">
                      <thead>
                        <tr>
                          @*<th> ID </th>*@
                          <th> Type of Solution </th>
                          <th> Short Cut</th>
                          <th>  </th>
                        </tr>
                      </thead>
                      <tbody>
                          @foreach(var solutionsType in Model){
                          <tr>             
                          @*<td> @category_partner.id </td>*@
                          <td> @solutionsType.type_name </td>
                          <td> @solutionsType.short_cut</td>
                          <td> <a href="UpdateSolutionType\@solutionsType.id"><button type="button" class="btn btn-inverse-info btn-sm">Edit</button></a>
							  </td>
                          </tr>
                          }
                      </tbody>
                    </table>
                    
                  </div>
                </div>
              </div>
