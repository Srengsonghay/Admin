@model IEnumerable<Admin.ViewModel.UserViewModel>
@{
	ViewData["Title"] = "Users";
	Layout="_LayoutDash";
}
<partial name="_AdminPartial" />
<div class="col-lg-12 grid-margin stretch-card" style="margin-top:0px;">
	<div class="card">
         <div class="card-body">
                     
			<h1 class="card-title">Users</h1>
			<table class="table">
				<thead>
					<tr>
						<th>
							@Html.DisplayNameFor(model => model.UserName)
						</th>
						<th>
							@Html.DisplayNameFor(model => model.Email)
						</th>
						<th>

						</th>
					</tr>
				</thead>
				<tbody>
					@foreach (var item in Model)
					{
						<tr>
							<td>
								@Html.DisplayFor(modelItem => item.UserName)
							</td>
							<td>
								@Html.DisplayFor(modelItem => item.Email)
							</td>
							<td>
								<a asp-action="EditUser" asp-controller="Admin" asp-route-id="@item.Id">Edit</a>
							</td>
						</tr>
					}
				</tbody>
			</table>
			</div>
		</div>
</div>